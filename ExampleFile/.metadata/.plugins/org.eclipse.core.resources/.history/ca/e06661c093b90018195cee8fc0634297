 import java.net.*;
import java.io.*;
public class A {
	public static void main(String[] args) throws IOException {
		URL u1=new URL("https://pagalworld3.net/8489/Kesariya%20Balam%20(Coke%20studio).mp3");
		/*System.out.println(u1.getProtocol());
		System.out.println(u1.getHost());
		System.out.println(u1.getPort());
		System.out.println(u1.getFile());
		System.out.println(u1.toExternalForm());
		System.out.println(u1);*/
		InputStream fis=u1.openStream();
		FileOutputStream fos= new FileOutputStream("d:/Kesariya_Balam.mp3");
		int n;
		while ((n=fis.read())!= -1) {
			fos.write(n);
			
		}
		fis.close();
		fos.close();
	}

}
